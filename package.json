{
  "name": "yt-bookmarks",
  "displayName": "Yt bookmarks",
  "version": "0.0.1",
  "description": "This will save bookmarks of any youtube video and when you open that video you can see them by clicking on the extension icon in upperbar ",
  "author": "Muhammad Ahmad",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.12.0",
    "plasmo": "0.89.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://www.youtube.com/*"
    ]
  }
}
